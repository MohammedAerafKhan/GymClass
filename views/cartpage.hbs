<main id="cartpage">
    {{#if loggedIn}}
        {{#if isCartEmpty}}
            <h1>Your cart is empty at the moment!</h1>
        {{else}}
            <h2>Your Cart</h2>
            {{#each cartItems}}
                <section>
                    <p>Booked class name: {{this.className}}</p>
                    <p>duration: {{this.lengthofCLass}}</p>
                    <p>Subtotal: {{this.subTotal}}</p>
                    <p>Totalcost: {{this.totalcost}}</p>
                </section>
            {{/each}}
        {{/if}}
    {{else}}
        <h1>You must be logged in to see the cart</h1>
    {{/if}}
</main>
